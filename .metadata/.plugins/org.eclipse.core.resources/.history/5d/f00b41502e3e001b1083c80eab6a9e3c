*/
package maquinaGolosinas;

import java.util.ArrayList;
import java.util.List;

public class IDExis {
	boolean siEsta=false;
	List<String> idProd = new ArrayList<String>() {
		{
			add("A2");
			add("A4");
			add("A8");
			add("B1");
			add("B6");
			add("B3");
			add("C5");
			add("C7");
			add("C9");
			add("D1");
			add("D4");
			add("D2");
		}
	};
	public boolean Existe(String id) {
		if(idProd.contains(id)) {
			siEsta=true;
		}
		return siEsta;
	}

}
